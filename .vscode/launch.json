{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run pytest",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "./src"
            },
            "envFile": "${workspaceRoot}/.env",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: CDK Synth",
            "type": "python",
            "request": "launch",
            "program": "./app.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: Run Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "./src"
            }
        },
    ]
}
